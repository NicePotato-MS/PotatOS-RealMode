{
    "scopeName": "source.frenchfries",
    "fileTypes": ["frfr"],
    "name": "FrenchFries",
    "patterns": [
		{
            "include": "#comments"
        },
        {
            "include": "#brackets"
        },
        {
            "include": "#autoClosingPairs"
        },
        {
            "include": "#surroundingPairs"
        },
        {
            "match": "\\b(if|then|else|while|do|for|end|return|function)\\b",
            "name": "keyword.control.frenchfries"
        },
        {
            "match": "\\b(byte|word|dword|qword|string)\\b",
            "name": "storage.type.frenchfries"
        },
        {
            "match": "\\b(true|false)\\b",
            "name": "constant.language.boolean.frenchfries"
        },
        {
            "match": "\\b(>|<|>=|<=|==|!=)\\b",
            "name": "keyword.operator.comparison.frenchfries"
        },
        {
            "match": "\\b(or|and|not|\\+|-|\\\\|\\*|\\^)\\b",
            "name": "keyword.operator.logical.frenchfries"
        },
        {
            "begin": "\"",
            "end": "\"",
            "name": "string.quoted.double.frenchfries"
        },
        {
            "begin": "'",
            "end": "'",
            "name": "string.quoted.single.frenchfries"
        },
        {
            "match": "--[\\s\\S]*?[^\\\\]$",
            "name": "comment.line.double-dash.frenchfries"
		},
		{
            "name": "constant.numeric.frenchfries",
            "match": "\\b\\d+\\b"
        },
		{
            "name": "constant.numeric.hex.frenchfries",
            "match": "\\b0x[0-9A-Fa-f]+\\b"
        },
		{
            "name": "support.function.lua",
            "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b\\s*\\("
        },
		{
            "name": "variable.other.frenchfries",
            "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
        }
    ],
    "repository": {},
    "uuid": "fd0d375e-87f0-4bdd-b4ce-8f58c3711ead"
}